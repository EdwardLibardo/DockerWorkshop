pipeline {
    agent any 
    stages {
        stage('preparación') { 
            steps {
                 echo 'Hello world!' 
            }
        }
        stage('construcción') { 
            steps {
                echo 'Construccion' 
            }
        }
        stage('análisis de la imagen,') { 
            steps {
                echo 'Imagen!' 
            }
        }
    }
}
